{
  "presets": [
    ["env", {
      "modules": false,
      "loose": true,
      "targets": {
        "browsers": [
          "> 5%",
          "last 2 versions",
          "not ie <= 10"
        ]
      }
    }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-pug-html",
    "transform-async-to-generator"
  ]
}